# Example environment variables for frontend
# Copy this file to .env and fill in your values

# WebSocket URL for backend connection
# For local development:
# VITE_WS_URL=ws://localhost:8080/connection

# For production (Railway, Render, etc.):
# You can use either format - the app will auto-convert:
# VITE_WS_URL=https://your-backend.railway.app/connection
# OR
# VITE_WS_URL=wss://your-backend.railway.app/connection

VITE_WS_URL=ws://localhost:8080/connection
